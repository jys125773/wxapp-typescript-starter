<view class="ui-swipeout custom-class">
  <view class="ui-swipeout__view" bind:touchstart="{{ disabled ? '' : 'bindTouchStart' }}" bind:touchmove="{{ disabled ? '' : 'bindTouchMove' }}" bind:touchend="{{ disabled ? '' : 'bindTouchEnd' }}" style="-webkit-transform:translateX({{ -offsetX }}px);transform:translateX({{ -offsetX }}px);{{ touchMoving ? '' : 'transition:transform ' + transitionDration + 'ms;' + '-webkit-transition:transform ' + transitionDration + 'ms;'  }}">
    <view class="ui-swipeout__left">
      <slot name="left"></slot>
    </view>
    <view class="ui-swipeout__content" bind:tap="close">
      <slot></slot>
    </view>
    <view class="ui-swipeout__right">
      <slot name="right"></slot>
    </view>
  </view>
</view>
